CREATE TABLE IF NOT EXISTS `opset_option` (
  `opset_option_id` int(11) NOT NULL AUTO_INCREMENT,
  `opset_id` int(11) DEFAULT NULL,
  `option_id` int(11) DEFAULT NULL,
  PRIMARY KEY (`opset_option_id`),
  KEY `fk_opset_option_1_idx` (`opset_id`),
  KEY `fk_opset_option_2_idx` (`option_id`),
  CONSTRAINT `fk_opset_option_1` FOREIGN KEY (`opset_id`) REFERENCES `opset` (`opset_id`) ON DELETE CASCADE ON UPDATE CASCADE,
  CONSTRAINT `fk_opset_option_2` FOREIGN KEY (`option_id`) REFERENCES `option` (`option_id`) ON DELETE NO ACTION ON UPDATE NO ACTION
) ENGINE=InnoDB DEFAULT CHARSET=latin1;
